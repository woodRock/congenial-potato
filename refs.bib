@ARTICLE{Raman2023,
	author = {Raman, R. K. and Das, Archan Kanti and Manna, Ranjan K. and Sahu, Sanjeev Kumar and Das, Basanta K.},
	title = {Ability of machine learning models to identify preferred habitat traits of a small indigenous fish (Chanda nama) in a large river of peninsular India},
	year = {2023},
	journal = {Environmental Science and Pollution Research},
	volume = {30},
