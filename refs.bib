  month={},}

@ARTICLE{Mayormente2024,
	author = {Mayormente, Marvin D.},
	title = {Intelligent Recirculating Aquaculture System of Oreochromis Niloticus: A Feed-Conversion-Ratio-Based Machine Learning Approach},
