}

@inproceedings{Zhao2023a,
  author    = {Zhao, J. and Xue, B. and Vennell, R. and Zhang, M.},
  title     = {{Large-Scale Mussel Farm Reconstruction with GPS Auxiliary}},
  booktitle = {International Conference on Image and Vision Computing New Zealand (IVCNZ)},
  year      = {2023},
  abstract = {The expanding mussel farming industry, especially in regions like New Zealand (NZ), necessitates advanced monitoring and management solutions for sustainable and efficient operations. Currently, manual labor and on-site operations limit real-time capabilities and overall efficiency. To address these challenges, large-scale 3D reconstruction offers a viable solution, enabling reproducible representations of mussel farm scenes based on captured video for off-site evaluation and precise decision-making. Conventional 3D reconstruction methods often require intricate manual camera calibration, posing challenges for real-world applications, particularly in marine scenarios. In contrast, this paper presents an innovative and practical approach for large-scale mussel farm reconstruction by integrating GPS auxiliary data without heavy camera calibration. The primary objective is to develop a comprehensive large-scale 3D visualization of mussel farms using video footage and simultaneous GPS information captured by a vessel-mounted camera. Our methodology involves a two-step process: First, making adaptive image sampling from captured video and applying Structure from Motion (SfM) for camera pose estimation and a dense 3D reconstruction, accurately reproducing local 3D scenes of partial mussel farms. Then, GPS data will be integrated for georeferencing and localization. This alignment empowers farm managers with geospatial accuracy for decision-making, resource allocation, and strategic planning. Experimental results validate the effectiveness of the proposed approach on reconstruction accuracy and completeness, and GPS geo-registration precision, highlighting its potential for advancing mussel farm monitoring.}
}

@inproceedings{Zhao2023b,
  author    = {Zhao, J. and McMillan, C. and Xue, B. and Vennell, R. and Zhang, M.},
  title     = {{Buoy Detection under Extreme Low-light Illumination for Intelligent Mussel Farming (Best Paper Award)}},
  booktitle = {International Conference on Image and Vision Computing New Zealand (IVCNZ)},
  year      = {2023},
  abstract = {Mussel farming in New Zealand (NZ) is a thriving and crucial industry that supports local communities and underscores aquaculture’s significance. A key task in mussel farming involves buoy inspection to monitor mussel growth for efficient harvesting. Currently, this process heavily relies on manual labor, resulting in labor and time-intensive spot operations for farmers. To address this, AI methods have recently been studied for fully automated buoy detection from captured images, showing promising results. However, these methods have mainly focused on images with good illumination, limiting their applicability in low-light conditions, such as dawn and dusk. This paper presents the first exploration of AI-based automated buoy detection in low-light environments, taking steps toward full-day buoy inspection. We investigate the impact of low-light conditions on deep learning-based buoy detection performance and explore techniques like intrinsic decomposition and low-light image enhancement to mitigate these effects. Incorporating these analyzed and enhanced results into a YOLO-based deep learning model results in significant improvements in low-light detections.}
}

@article{Zhao2025,
  author  = {Zhao, J. and Xue, B. and Vennel, R. and Zhang, M.},
  title   = {{Hierarchical mussel farm reconstruction from video with object tracking}},
  journal = {Journal of the Royal Society of New Zealand},
  year    = {2025},
  volume  = {55},
  number  = {6},
  pages   = {1563--1588},
  doi     = {10.1080/03036758.2024.2345316},
  abstract = {As the mussel farming industry expands, particularly in regions such as New Zealand, there is a growing need for advanced monitoring and management solutions to ensure sustainability and operational efficiency. The current reliance on manual infrequent observation of aquaculture structures limits farmers' ability to monitor them in real time. Addressing these challenges, large-scale 3D reconstruction provides a practical solution by facilitating the creation of replicable depictions of mussel farm scenes and floatation buoys derived from recorded video, supporting off-site evaluation and enabling precise decision making. This paper introduces a novel approach to enhance the visualisation and monitoring of mussel farm floatation through a hierarchical reconstruction process. In contrast to earlier studies, we focus on recovering not only the overall environment (background) but also the finer details of key elements such as buoys to create a comprehensive representation of mussel farm geometry and appearance. We propose to segment the scene into the background and granular object instances and reconstruct them separately in a multi-stage process. The initial 3D scene reconstruction is performed using the Structure-from-Motion (SfM) technique, leveraging video footage captured by a vessel-mounted camera. This coarse reconstruction serves as the foundation for subsequent fine-grained enhancements. To recover finer details, object tracking is applied and the trajectories obtained are then conjunct with geometry triangulation to determine the real-world positions of individual buoys. A multiple-scale denoising method, grounded in dominant direction correlation, is implemented to eliminate non-reliable tracking objects and reduce reconstruction artifacts, ensuring the accuracy of the final results. This hierarchical reconstruction approach contributes to the advancement of mussel farm management by offering a powerful technology for comprehensive visualisation, enabling farmers to make informed decisions based on a detailed understanding of the mussel farm's geometry and dynamics.}
}

@article{Zhou2024,
  author  = {Zhou, Y. and Chen, G. and Xue, B. and Zhang, M. and Rooney, J. S. and Lagutin, K. and Killeen, D. P.},
  title   = {{Machine Learning for Raman Spectroscopy-based Cyber-Marine Fish Biochemical Composition Analysis}},
  journal = {arXiv preprint arXiv:2409.19688},
  year    = {2024},
  doi     = {10.48550/arxiv.2409.19688},
  abstract = {The rapid and accurate detection of biochemical compositions in fish is a crucial real-world task that facilitates optimal utilization and extraction of high-value products in the seafood industry. Raman spectroscopy provides a promising solution for quickly and non-destructively analyzing the biochemical composition of fish by associating Raman spectra with biochemical reference data using machine learning regression models. This paper investigates different regression models to address this task and proposes a new design of Convolutional Neural Networks (CNNs) for jointly predicting water, protein, and lipids yield. To the best of our knowledge, we are the first to conduct a successful study employing CNNs to analyze the biochemical composition of fish based on a very small Raman spectroscopic dataset. Our approach combines a tailored CNN architecture with the comprehensive data preparation procedure, effectively mitigating the challenges posed by extreme data scarcity. The results demonstrate that our CNN can significantly outperform two state-of-the-art CNN models and multiple traditional machine learning models, paving the way for accurate and automated analysis of fish biochemical composition.}
}

@CONFERENCE{Zhou2025,
	author = {Zhou, Xiaoteng and Wang, Yuanbo and Ding, Yutian and Wang, Jialu and Mizuno, Katsunori and Tabeta, S.},
	title = {Investigating the Ecological Impacts of Marine Debris on Fish Habitats Using High-Resolution Acoustic Cameras and Ai-Driven Analysis},
	year = {2025},
	journal = {Proceedings of the International Conference on Offshore Mechanics and Arctic Engineering - OMAE},
	volume = {2},
	pages = {},
	doi = {10.1115/OMAE2025-155118},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-105015147109&doi=10.1115%2FOMAE2025-155118&partnerID=40&md5=62a3bd65ca3e71810ff180a91b6736e8},
	abstract = {Marine debris, as a negative externality, is often regarded as a threat to aquatic ecosystems. However, its impact is not exclusively negative; some debris can function as artificial habitats under certain conditions, providing ecological benefits. Verifying this hypothesis requires long-Term monitoring and systematic analysis. Given the low-visibility nature of most aquatic environments, traditional observational methods face significant limitations in uncovering the potential ecological impacts of marine debris. This study proposes an integrated monitoring framework that combines high-resolution acoustic imaging technology and machine learning algorithms to investigate the ecological effects of debris on fish behavior. Specifically, acoustic visual data on debris distribution and fish behavior are collected using a single acoustic camera. Machine learning algorithms are then employed to enhance the monitoring capability of the framework to distinguish between fish and debris targets. Finally, the optical flow technique was applied to visualize fish behavior trajectories around the debris. The approach was validated through water tank experiments and field tests, demonstrating its feasibility and robustness. © 2025 Elsevier B.V., All rights reserved.},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0; All Open Access; Gold Open Access}
}

